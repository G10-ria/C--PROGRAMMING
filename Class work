#include<iostream>
#include<string>

using namespace std;
int main(){
string name;
int age;

//prompt the user for their name
cout<<"Enter your name:";
cin>>name;

//prompt the user for their age
cout<<"Enter your age:";
cin>>age;

if(age>=18){
cout<<"You are eligible to voting"<<name<<".";
  }else{
cout<<"You are not eligible for voting"<<name<<".";
}
return 0;
}
